/******* Start of generated part using types */
<%_ types.filter(type => !type.isJoinTable).forEach(type => { -%>
import { handler<%= type.name%> } from './handlers/handler<%= type.name%>'
<% }); %>
/******* End of generated part using types */

module.exports = {

	handleGet: (args, type, directives) => {
		switch (type) {
			/******* Start of generated part using types */
			<% types.filter(table => !table.isJoinTable).forEach(table => { %>
			case "<%= table.name %>Type":
				if(args) {
					return handler<%= table.name %>.getMethodsByArgs(args, directives)
				}
				else {
					return handler<%= table.name %>.getMethods()
				}
			<% }); %>
			/******* End of generated part using types */
			default:
			break
		}
	},

	handleDelete: (id, type , directives) => {
